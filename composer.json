{
    "name": "davahome/php-cs-fixer-ruleset",
    "description": "The default ruleset of php-cs-fixer for all davahome projects",
    "type": "library",
    "scripts": {
        "phpcs": [
            "bash bin/php-cs-fixer.sh",
            "@php php-cs-fixer.phar fix --config=php_cs.php"
        ]
    },
    "autoload": {
        "classmap": [
            "lib/PhpCsFixer.php"
        ]
    }
}
